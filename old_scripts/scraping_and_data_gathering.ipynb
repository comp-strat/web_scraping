    "def searchText(keywordList, passage):\n",
    "    for i in range(len(keywordList)):\n",
    "        if not isinstance(passage, list):\n",
    "            passage = passage.split(keywordList[i])\n",
    "        elif len(passage) == 1:\n",
    "            passage = passage[0].split(keywordList[i])\n",
    "        if len(passage) > 1:\n",
    "            return True\n",
    "    return False\n",
    "def searchTextForNumbers(passage):\n",
    "    passage = passage + 'a'\n",
    "    currentNumber = '0'\n",
    "    for char in passage:\n",
    "        try:\n",
    "            int(char)\n",
    "            currentNumber += char\n",
    "        except:\n",
    "            currentNumber = int(currentNumber)\n",
    "            if isYear(currentNumber):\n",
    "                return True\n",
    "            currentNumber = '0'\n",
    "    return False\n",
    "def isYear(number):\n",
    "    if number >= 1900 and number <=2012:\n",
    "        return True\n",
    "    return False\n",
    "def mapper(txt):\n",
    "    print(txt.upper())\n",
    "    text = txt.upper()\n",
    "    bufferText = text\n",
    "    if (searchText(mission,bufferText)): centroidmodel['mission'] += [txt]\n",
    "    bufferText = text\n",
    "    if (searchText(curriculum,bufferText)): centroidmodel['curriculum'] += [txt]\n",
    "    bufferText = text\n",
    "    if (searchText(philosophy,bufferText)): centroidmodel['philosophy'] += [txt]\n",
    "    bufferText = text\n",
    "    if (searchText(history,bufferText) or searchTextForNumbers(bufferText)): centroidmodel['history'] += [txt]\n",
    "    bufferText = text\n",
    "    if (searchText(target,bufferText)): centroidmodel['target'] += [txt]\n",
    "    bufferText = text\n",
    "    if (searchText(resources,bufferText)): centroidmodel['resources'] += [txt]\n",
    "    bufferText = text\n",
    "    if (searchText(orgfactor,bufferText)): centroidmodel['organizational_factors'] += [txt]\n",
   ]
